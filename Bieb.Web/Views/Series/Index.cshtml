@model IEnumerable<Bieb.Domain.Entities.Series>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Title
        </th>
        <th>
            Books
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.Title, "Details", new { id=item.Id })
        </td>
        <td>
            @string.Join(", ", item.Books.Values)
        </td>
    </tr>
}

</table>
